  "paths": {
    "/companies/{companyId}/reports/spendAnalysis": {
      "parameters": [
        {
          "$ref": "#/components/parameters/companyId"
        }
      ],
      "post": {
        "summary": "Create spend analysis report",
        "description": "Create spend analysis report (no body required in POST).",
        "operationId": "create-spend-analysis-report",
        "tags": [
          "Spend analysis"
        ],
        "parameters": [
          {
            "schema": {
              "$ref": "#/components/schemas/DateTime"
            },
            "in": "query",
            "name": "from",
            "description": "The date the report was created from."
          },
          {
            "schema": {
              "$ref": "#/components/schemas/DateTime"
            },
            "in": "query",
            "name": "to",
            "description": "The date the report was generated to."
          },
          {
            "schema": {
              "type": "number",
              "format": "decimal",
              "default": 1
            },
            "in": "query",
            "name": "excludeSuppliersWithAnnualisedSpendBelow",
            "description": "Exclude suppliers with annualised spend below this amount in base currency."
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ReportMetadata"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "402": {
            "$ref": "#/components/responses/Payment-Required"
          },
          "403": {
            "$ref": "#/components/responses/Forbidden"
          },
          "404": {
            "$ref": "#/components/responses/Not-Found"
          },
          "429": {
            "$ref": "#/components/responses/Too-Many-Requests"
          },
          "500": {
            "$ref": "#/components/responses/Internal-Server-Error"
          },
          "503": {
            "$ref": "#/components/responses/Service-Unavailable"
          }
        }
      },
      "get": {
        "summary": "Get created spend analysis report",
        "description": "Get spend analysis report that has already been successfully created.",
        "operationId": "get-spend-analysis-report",
        "tags": [
          "Spend analysis"
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SpendAnalysisWithSummaries"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "402": {
            "$ref": "#/components/responses/Payment-Required"
          },
          "403": {
            "$ref": "#/components/responses/Forbidden"
          },
          "404": {
            "$ref": "#/components/responses/Not-Found"
          },
          "429": {
            "$ref": "#/components/responses/Too-Many-Requests"
          },
          "500": {
            "$ref": "#/components/responses/Internal-Server-Error"
          },
          "503": {
            "$ref": "#/components/responses/Service-Unavailable"
          }
        }
      }
    },
    "/companies/{companyId}/reports/{reportType}/status": {
      "parameters": [
        {
          "$ref": "#/components/parameters/companyId"
        },
        {
          "$ref": "#/components/parameters/reportType"
        }
      ],
