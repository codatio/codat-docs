{
  "version": "0.2",
  "language": "en",
  "files": ["**/*.md", "**/*.mdx"],
  "exclude": ["**/node_modules/**", "**/dist/**", "**/.git/**"],
  "ignorePaths": [
    "package-lock.json",
    "yarn.lock",
    "node_modules/**",
    "**/README.md"
  ],
  "patterns": [
    {
      "name": "match_markdown_frontmatter",
      "pattern": "/^(-{3}|\\[+\\]{3})$(\\s|\\S)*?^\\1$/gm"
    },
    {
      "name": "match_markdown_code_blocks",
      "pattern": "/^```[\\s\\S]*?^```$/gm"
    },
    {
      "name": "match_markdown_inline_code",
      "pattern": "/`[^`]+`/g"
    }
  ],
  "languageSettings": [
    {
      "languageId": "markdown",
      "ignoreRegExpList": [
        "match_markdown_frontmatter",
        "match_markdown_code_blocks",
        "match_markdown_inline_code"
      ]
    }
  ],
