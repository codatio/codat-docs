{
  "version": "0.2",
  "language": "en",
  "files": ["**/*.md", "**/*.mdx"],
  "exclude": ["**/node_modules/**", "**/dist/**", "**/.git/**"],
  "ignorePaths": [
    "package-lock.json",
    "yarn.lock",
    "node_modules/**"
  ],
  "patterns": [
    {
      "name": "match_markdown_frontmatter",
      "pattern": "/^(-{3}|\\[+\\]{3})$(\\s|\\S)*?^\\1$/gm"
    }
  ],
  "languageSettings": [
    {
      "languageId": "markdown",
      "ignoreRegExpList": ["match_markdown_frontmatter"]
    }
